{
  "name": "skin-retoucher-pro",
  "version": "1.0.0",
  "description": "A professional-grade desktop application for skin retouching.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.electron.skinretoucher",
    "productName": "Skin Retoucher Pro",
    "asar": true,
    "asarUnpack": null,
    "compression": "maximum",
    "files": [
      "main.js",
      "preload.js",
      "renderer.js",
      "webgl-utils.js",
      "index.html",
      "style.css",
      "enhance.png",
      "Skin Retoucher.png"
    ],
    "win": {
      "icon": "enhance.png",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "keywords": [
    "electron",
    "webgl",
    "image-processing",
    "retouching"
  ],
  "author": "Eddy Kuo",
  "license": "ISC",
  "devDependencies": {
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12"
  }
}